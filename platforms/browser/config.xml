<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tuCanchaApp" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TuCancha App</name>
    <description>
        soccer fild reservation app
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        TuCancha App Team
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <icon platform="android" qualifier="ldpi" src="icon.png" />
    <icon platform="android" qualifier="mdpi" src="icon.png" />
    <icon platform="android" qualifier="hdpi" src="icon.png" />
    <icon platform="android" qualifier="xhdpi" src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*://*api.parse.com/*" />
</widget>
