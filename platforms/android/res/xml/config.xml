<?xml version='1.1' encoding='utf-8'?>
<widget id="com.tuCanchaApp" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/2.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>TuCancha App</name>
    <description>
        soccer fild reservation app
    </description>
    <author email="sebasdeldi123@gmail.com" href="http://www.tucanchaapp.com/">
        TuCancha App Team
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <icon platform="android" qualifier="ldpi" src="icon_ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="icon_mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="icon_hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="icon_xhdpi.png" />
    <icon platform="android" qualifier="xxhdpi" src="icon_xxhdpi.png" />
    <icon platform="android" qualifier="xxxhdpi" src="icon_xxxhdpi.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*://*api.parse.com/*" />
</widget>
