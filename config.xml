<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tuCanchaApp" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/2.0">
    <name>TuCancha App</name>
    <description>
        soccer fild reservation app
    </description>
    <author email="sebasdeldi123@gmail.com" href="http://www.tucanchaapp.com/">
        TuCancha App Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-geolocation" version="1" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <icon src="icon.png" />
    <icon src="icon_ldpi.png" platform="android" qualifier="ldpi" />
    <icon src="icon_mdpi.png" platform="android" qualifier="mdpi" />
    <icon src="icon_hdpi.png" platform="android" qualifier="hdpi" />
    <icon src="icon_xhdpi.png" platform="android" qualifier="xhdpi" />
    <icon src="icon_xxhdpi.png" platform="android" qualifier="xxhdpi" />
    <icon src="icon_xxxhdpi.png" platform="android" qualifier="xxxhdpi" />
 
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*://*api.parse.com/*"/>

    <platform name="android">
        <allow-intent href="market:*" />

    </platform>

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
